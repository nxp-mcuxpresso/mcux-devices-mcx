# core related
config MCUX_HW_CORE
    string
    default "cm33"

config MCUX_HW_CORE_CM33
    bool
    default y

config MCUX_HW_CORE_ID
    string
    default "cm33"

config MCUX_HW_CORE_ID_CM33
    bool
    default y

config MCUX_HW_DEVICE_CORE
    string
    default "MCXL142"

config MCUX_HW_FPU
    string
    default "NO_FPU"

config MCUX_HW_FPU_TYPE
    string
    default "no_fpu"

config MCUX_HW_NO_FPU
    bool
    default y

config MCUX_HW_INT_PRIO_BITS
    int
    default 3

config MCUX_HW_DSP
    bool
    default n

config MCUX_HW_SAU
    bool
    default n

config MCUX_HW_SOC_SERIES_MCX
    bool
    default y

config MCUX_HW_SOC_SINGLECORE_DEVICE
    bool
    default y

# device related

config MCUX_HW_DEVICE_ID_MCXL142xxx
    bool

choice
    prompt "Device MCXL142 Part"
    config MCUX_HW_DEVICE_PART_MCXL142VLH
    bool "Device part MCXL142VLH"
    select MCUX_HW_DEVICE_ID_MCXL142xxx
    config MCUX_HW_DEVICE_PART_MCXL142VPJ
    bool "Device part MCXL142VPJ"
    select MCUX_HW_DEVICE_ID_MCXL142xxx
endchoice

config MCUX_HW_DEVICE_ID
    string
    default "MCXL142xxx" if MCUX_HW_DEVICE_ID_MCXL142xxx

config MCUX_HW_DEVICE_PART
    string
    default "MCXL142VLH" if MCUX_HW_DEVICE_PART_MCXL142VLH
    default "MCXL142VPJ" if MCUX_HW_DEVICE_PART_MCXL142VPJ

# For chipmodel generated files
config MCUX_TOOLCHAIN_LINKER_DEVICE_PREFIX
    string
    default "MCXL142" if MCUX_HW_DEVICE_ID_MCXL142xxx

config MCUX_TOOLCHAIN_MCUX_STARTUP
    string
    default "mcxl142"

# Toolchain related

config MCUX_TOOLCHAIN_IAR_CPU_IDENTIFIER
    string
    default "MCXL142xxx" if MCUX_HW_DEVICE_ID_MCXL142xxx

config MCUX_TOOLCHAIN_MDK_CPU_IDENTIFIER
    string
    default "MCXL142VLH" if MCUX_HW_DEVICE_PART_MCXL142VLH
    default "MCXL142VPJ" if MCUX_HW_DEVICE_PART_MCXL142VPJ

config MCUX_TOOLCHAIN_JLINK_CPU_IDENTIFIER
    string
    default "MCXL142xxx" if MCUX_HW_DEVICE_ID_MCXL142xxx

